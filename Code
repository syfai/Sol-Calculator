// Create a contract called Calculator
// Add a variable to store result
// Add functions for adding, subtracting, and multiplying
// Add the final funtion to get the result

pragma solidity ^0.8.18;

contract Calculator {
    uint256 result = 0;

    function add(uint256 num) public {
        result += num; 
    }
    function subtract(uint256 num) public {
        result -= num;
    } 
    function multiply(uint256 num) public {
        result *= num;
    }
    function get() public view returns(uint256) {
        return result;
    }
}
